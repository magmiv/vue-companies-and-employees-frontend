{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "trailing-comma": [
      false
    ],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "array-type": [
      true,
      "array"
    ],
    "member-ordering": false,
    "quotemark": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
